    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to remote",
            "protocol": "inspector",
            "address": "localhost",
            "port": 9229,
            "sourceMaps": true,
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "/batcher",
            "sourceMapPathOverrides": {
                "/usr/src/app/*": "${workspaceRoot}/*"
            }
        }
    ]
}